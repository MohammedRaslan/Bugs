        function ShopItemDetailsPage(navCtrl, router, route, toast) {
          var _this = this;

          _classCallCheck(this, ShopItemDetailsPage);

          this.navCtrl = navCtrl;
          this.router = router;
          this.route = route;
          this.toast = toast;
          this.item_number = 1;
          this.route.queryParams.subscribe(function (params) {
            if (_this.router.getCurrentNavigation().extras.state) {
              _this.item = _this.router.getCurrentNavigation().extras.state.item;
              console.log(_this.item);
